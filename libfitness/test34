int
main() {
  int a[2];
  int b[2, 3];

  a[1] = 1;
  b[1,2] = 2;

  print << "a:\n";
  for(int i=0; i<2; i++) {
    print << i << ": " << a[i] << "\n";
  }

  print << "\nb:\n";
  for(int i=0; i<2; i++) {
    for(int j=0; j<3; j++) {
      print << i << "," << j << ": " << b[i,j] << "\n";
    }
  }
}

