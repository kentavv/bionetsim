  k1 = 0.1
  k2 = 0.1
  k3 = 0.1
  k4 = 0.01
  k4r = 1
  k5 = 1
  k6 = 0.01
  k6r = 1
  k7 = 1
  k8 = 1

  R1 = -4 * k6 * R1**4 * b + 4 * k6r * (1-b) + k8 * a - k3 * R1

  R2 = -4 * k4 * a * R2**4 + 4 * k4r * (1-a) + k7 * b - k1 * R2

  Z = +k5 * a - k2 * Z

  a = -k4 * a * R2**4 + k4r * (1-a) - k5 * a + k5 * a - k8 * a + k8 * a

  b = -k6 * R1**4 * b + k6r * (1-b) - k7 * b + k7 * b

