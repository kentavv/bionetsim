MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-gui \
		sub-nongui

first: all

all: Makefile $(SUBTARGETS)

gui/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "gui" || $(MKDIR) "gui"
	cd gui && $(QMAKE) gui.pro -o $(MAKEFILE)
sub-gui: gui/$(MAKEFILE) FORCE
	cd gui && $(MAKE) -f $(MAKEFILE)

nongui/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "nongui" || $(MKDIR) "nongui"
	cd nongui && $(QMAKE) nongui.pro -o $(MAKEFILE)
sub-nongui: nongui/$(MAKEFILE) FORCE
	cd nongui && $(MAKE) -f $(MAKEFILE)

Makefile: libnetwork.pro  /usr/lib64/qt-3.3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile libnetwork.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile libnetwork.pro

all: $(SUBTARGETS)
qmake_all: gui/$(MAKEFILE) nongui/$(MAKEFILE)
	 ( [ -d gui ] && cd gui ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d nongui ] && cd nongui ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d gui ] && cd gui ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d nongui ] && cd nongui ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d gui ] && cd gui ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d nongui ] && cd nongui ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d gui ] && cd gui ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d nongui ] && cd nongui ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d gui ] && cd gui ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d nongui ] && cd nongui ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

